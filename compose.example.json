{
    // {base} object owns all the options for the base generator
    "base": {
      "projects": [ // list of the needed generators (strings matches with the boilerplate's name)
        "express-typescript",
        "react-typescript",
        "express-vanilla",
        "react-vanilla"
      ],
      "rush": true // in case you want to use rush with all your newly generated projects
    },
    // {boilerplates} object owns all the options for each sub-generators
    "boilerplates": {
      "express-typescript": {
        "applicationName": "my-backend-typescript-app"
      },
      "express-vanilla": {
        "applicationName": "my-backend-app"
      },
      "react-typescript": {
        "applicationName": "my-frontend-typescript-app"
      },
      "react-vanilla": {
        "applicationName": "my-frontend-app"
      }
    }
  }
  /*
  Message for the boilerplate developers:
    > update this file each time you add a boilerplate to the generator
  */